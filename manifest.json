{
    "name": "LinkedIn",
    "version": "1.0",
    "manifest_version": 2,
    "description": "LinkedIn",
    "app": {
        "background": {
            "scripts": ["/js/background.js"]
        }
    },
    "permissions": [
        "fileSystem",
        "fullscreen",
        "storage",
        "videoCapture",
        "audioCapture",
        "unlimitedStorage",
        "notifications",
        "https://www.google.com/calendar/*",
        "http://10.16.22.32:3000/*"
    ],
    "icons" : {
        "16": "/images/linkedin_64x64.jpeg",
        "19": "/images/linkedin_64x64.jpeg",
        "48": "/images/linkedin_64x64.jpeg",
        "128": "/images/linkedin_64x64.jpeg"
    }
}
