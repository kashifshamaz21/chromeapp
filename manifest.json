{
    "name": "LinkedIn",
    "version": "1.0",
    "manifest_version": 2,
    "description": "LinkedIn",
    "app": {
        "background": {
            "scripts": ["/js/background.js"]
        }
    },
    "permissions": [
        "fileSystem",
        "fullscreen",
        "storage",
        "videoCapture",
        "audioCapture",
        "unlimitedStorage",
        "notifications",
        "https://www.google.com/calendar/*",
        "http://10.16.22.32:3000/*",
        "identity",
        "*://*.linkedin.com/",
        "*://m.c.lnkd.licdn.com/*",
        "*://s.c.lnkd.licdn.com/*"
    ],
    "icons" : {
        "16": "/img/linkedin_64x64.jpeg",
        "19": "/img/linkedin_64x64.jpeg",
        "48": "/img/linkedin_64x64.jpeg",
        "128": "/img/linkedin_64x64.jpeg"
    }
}
